{
  "name": "anki-analyzer",
  "version": "0.1.0",
  "description": "Command line app for analyzing anki decks",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "start": "npm run build && node ./dist/index.js"
  },
  "author": "Pavol Varga",
  "license": "MIT",
  "dependencies": {
    "commander": "11.1.0",
    "figlet": "1.7.0"
  },
  "devDependencies": {
    "@types/figlet": "1.5.7",
    "@types/node": "20.8.9",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
